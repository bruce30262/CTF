alias a32="qemu-arm -L /usr/arm-linux-gnueabi/"
alias a32hf="qemu-arm -L /usr/arm-linux-gnueabihf/"
alias a64="qemu-aarch64 -L /usr/aarch64-linux-gnu/"

alias a32-objdump="/usr/bin/arm-linux-gnueabi-objdump"
alias a32-objcopy="/usr/bin/arm-linux-gnueabi-objcopy"
alias a32-gcc="/usr/bin/arm-linux-gnueabi-gcc"
alias a32-g++="/usr/bin/arm-linux-gnueabi-g++"
alias a32-as="/usr/bin/arm-linux-gnueabi-as"
alias a32-ld="/usr/bin/arm-linux-gnueabi-ld"
alias a32-readelf="/usr/bin/arm-linux-gnueabi-readelf"

alias a32hf-objdump="/usr/bin/arm-linux-gnueabihf-objdump"
alias a32hf-objcopy="/usr/bin/arm-linux-gnueabihf-objcopy"
alias a32hf-gcc="/usr/bin/arm-linux-gnueabihf-gcc"
alias a32hf-g++="/usr/bin/arm-linux-gnueabihf-g++"
alias a32hf-as="/usr/bin/arm-linux-gnueabihf-as"
alias a32hf-ld="/usr/bin/arm-linux-gnueabihf-ld"
alias a32hf-readelf="/usr/bin/arm-linux-gnueabihf-readelf"

alias a64-objdump="/usr/bin/aarch64-linux-gnu-objdump"
alias a64-objcopy="/usr/bin/aarch64-linux-gnu-objcopy"
alias a64-gcc="/usr/bin/aarch64-linux-gnu-gcc"
alias a64-g++="/usr/bin/aarch64-linux-gnu-g++"
alias a64-as="/usr/bin/aarch64-linux-gnu-as"
alias a64-ld="/usr/bin/aarch64-linux-gnu-ld"
alias a64-readlef="/usr/bin/aarch64-linux-gnu-readelf"
